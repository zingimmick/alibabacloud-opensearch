{
    "name": "zing/alibabacloud-opensearch",
    "description": "Alibaba Cloud OpenSearch SDK for PHP",
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^8.5",
        "rector/rector": "^0.12"
    },
    "autoload": {
        "files": [
            "OpenSearch/Autoloader/Autoloader.php"
        ],
        "psr-4": {
            "Thrift\\": "OpenSearch/Thrift/",
            "OpenSearch\\Client\\": "OpenSearch/Client/",
            "OpenSearch\\Generated\\": "OpenSearch/Generated/",
            "OpenSearch\\Util\\": "OpenSearch/Util/"
        }
    }
}
